{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"include": ["index.js", "utils.js", "model", "routes"],
		"includePattern": ".+\\.js$"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"destination": "./docs",
		"recurse": true
	}
}